{{ form_start(form) }}
<div class="form-group">
	{{ form_label(form.name, null, {'attr': {'class': 'form-label'}}) }}
	{{ form_widget(form.name, {'attr': {'class': 'form-input'}}) }}
</div>
<div class="form-group">
	{{ form_label(form.setNum, null, {'attr': {'class': 'form-label'}}) }}
	{{ form_widget(form.setNum, {'attr': {'class': 'form-input'}}) }}
</div>
<div class="form-group">
	{{ form_label(form.year, null, {'attr': {'class': 'form-label'}}) }}
	{{ form_widget(form.year, {'attr': {'class': 'form-input'}}) }}
</div>
<div class="form-group">
	{{ form_label(form.imageUrl, null, {'attr': {'class': 'form-label'}}) }}
	{{ form_widget(form.imageUrl, {'attr': {'class': 'form-input'}}) }}
</div>

<div class="form-actions">
	<button type="submit" class="btn btn-primary">
		{{ button_label|default('Save') }}
	</button>
</div>
{{ form_end(form) }}
