<form method="post" action="{{ path('app_set_delete', {'id': set.id}) }}" onsubmit="return confirm('Are you sure you want to delete this set?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ set.id) }}">
	<button class="btn btn-small btn-danger">
		Delete Set
	</button>
</form>
